{
  "name": "es-injection",
  "version": "0.2.2",
  "description": "A modern, lightweight dependency injection library inspired by JSR-303 and Spring.",
  "homepage": "https://github.com/rraziel/es-injection",
  "bugs": "https://github.com/rraziel/es-injection/issues",
  "license": "MIT",
  "keywords": [
    "ecmascript",
    "dependency",
    "injection",
    "typescript",
    "decorator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rraziel/es-injection.git"
  },
  "main": "./dist/es-injection.js",
  "browser": "./dist/es-injection.min.js",
  "types": "./dist/es-injection.d.ts",
  "scripts": {
    "build": "webpack --display-error-details --mode development",
    "build:release": "webpack --display-error-details --mode production",
    "build:watch": "webpack --display-error-details --mode development --watch",
    "compile": "tsc -p .",
    "compile:watch": "tsc --watch -p .",
    "quality:lint": "tslint --project ./tsconfig.json --config ./conf/tslint.json",
    "test": "jest --no-cache --verbose",
    "test:coverage": "jest --no-cache --coverage",
    "test:watch": "jest --watch --no-cache --verbose"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/index.ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/jest-junit-reporter",
    "testRegex": "\\.spec\\.ts",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "dependencies": {
    "reflect-metadata": "0.1.12"
  },
  "devDependencies": {
    "@types/jest": "23.1.2",
    "codecov": "3.0.2",
    "jest": "23.2.0",
    "jest-create-mock-instance": "1.0.3",
    "jest-junit-reporter": "1.1.0",
    "ts-jest": "22.4.6",
    "ts-loader": "4.4.1",
    "ts-node": "7.0.0",
    "tslint": "5.10.0",
    "typescript": "2.9.2",
    "uglifyjs-webpack-plugin": "1.2.7",
    "webpack": "4.12.2",
    "webpack-cli": "3.0.8"
  }
}
